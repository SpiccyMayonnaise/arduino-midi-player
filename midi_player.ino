static const uint16_t Freq8[] = { 4186 , 4435 , 4699  , 4978 , 5274 , 5588 , 5920 , 6272 , 6645 , 7040 , 7459 , 7902 };


static const long double tempo = 10;

class Voice {
    private:
      uint16_t _pin;

      //The current index of the note
      uint64_t currentNote = 0;
      //The time of the next note      
      long double nextNote = 0;

      //The current MIDI note
      uint8_t _note = 0;

      const uint16_t* _trackStart;
      uint16_t trackLength;

      Tone tone;
    
    public:
      Voice(int pin, const uint16_t* trackStart) {
        _pin = pin;

        tone.begin(_pin);

        _trackStart = trackStart;
      }

      void Update(uint64_t currentTime) {
        while (nextNote < (long double)currentTime) {
          uint16_t data = pgm_read_word((uint16_t *)(_trackStart + currentNote));
          if ((data & 0b0000000010000000) == 0b0000000000000000) {
            // note off
            uint8_t note = (data & 0b0000000001111111);
            if (_note == note) {
              tone.stop();
            }

          } else if((data & 0b0000000011111111) == 0b0000000000000000) {
              // Do nothing
          
          } else {
            uint8_t note = (data & 0b0000000001111111);
            _note = note;
            uint16_t Freq = Freq8[note % 12] / ( 1 << (8-(note / 12)) );
            tone.play(Freq / 2);    
            Serial.println(note);

          }    
          
          long double Duration = (long double)(data >> 8);
          

          currentNote++;
          
          nextNote += (Duration * tempo);
        }
      }
};

Voice* voice1;
Voice* voice2;

void setup() 
{
  Serial.begin(9600);

  voice1 = new Voice(7, &Track1[0]);
  voice2 = new Voice(8, &Track2[0]);
}

void loop() 
{
  uint64_t currentTime = millis();
  voice1->Update(currentTime);
  voice2->Update(currentTime);
}